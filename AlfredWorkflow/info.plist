				<key>escaping</key>
				<integer>102</integer>
				<key>script</key>
				<string>[ -z $bundleId ] &amp;&amp; exit 1

[[ -z $windowTitle &amp;&amp; -z $windowId ]] &amp;&amp; {open -b $bundleId; quit 0}

[ $bundleId = com.google.Chrome ] &amp;&amp; windowTitle+=' - Google Chrome'

hs -A -c "hs.application.applicationsForBundleID('$bundleId')[1]:getWindow(${windowId:-_cli._args[6]}):${mod:-raise}()" -- $windowTitle

[ -z $tabIndex ] || osascript -l JavaScript -e "
let win = Application('$bundleId').windows[0];
switch ('$bundleId') {
	case 'com.apple.Safari':
		win.currentTab = win.tabs[$tabIndex];
		break;
	case 'com.google.Chrome':
		win.activeTabIndex = $tabIndex + 1;
		break;
	case 'com.googlecode.iterm2':
	case 'com.googlecode.iterm':
		win.tabs[$tabIndex].select();
		break;
}"

[ -z $mod ] &amp;&amp; open -b $bundleId</string>
