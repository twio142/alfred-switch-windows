				<string>ObjC.import('AppKit')
ObjC.import('stdlib')

function run(argv) {
	let a = Application.currentApplication();
	a.includeStandardAdditions = true;
	let items = ObjC.unwrap($.NSWorkspace.sharedWorkspace.runningApplications)
		.filter(app =&gt; app.activationPolicy == $.NSApplicationActivationPolicyRegular &amp;&amp; !ObjC.unwrap(app.backgroundOnly))
		.map(app =&gt; {
			let path = ObjC.unwrap(app.bundleURL.fileSystemRepresentation);
			return {
				title: ObjC.unwrap(app.localizedName),
				subtitle: path,
				arg: path,
				variables: {pid: ObjC.unwrap(app.processIdentifier)},
				match: matchStr(path) || ObjC.unwrap(app.localizedName),
				icon: { path, type: 'fileicon' },
				type: 'file'
			}
		});

	function matchStr(filePath) {
		filePath = filePath.replace(/["\\]/g, '\\$&amp;');
		let db = $.getenv('alfred_workflow_data').replace(/[^\/]+\/[^\/]+$/, 'Databases/filecache.alfdb');
		let sql	= `sqlite3 "${db}" &lt;&lt; EOF
SELECT name || IFNULL(altnames, "") || " " || IFNULL(nameChars, "")
FROM files
WHERE path LIKE "%${filePath}"
LIMIT 1
EOF`;
		return a.doShellScript(sql);
	}

	return JSON.stringify({items}, null, 2);
}</string>
